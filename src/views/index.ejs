<html>
<head>
<title>Login Form</title>
<!-- bower:css -->
<link rel="stylesheet" href="/lib/font-awesome/css/font-awesome.min.css" />
<link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="/lib/chartist/dist/chartist.css" />
<!-- endbower -->
<!-- bower:js -->
<script src="/lib/jquery/dist/jquery.js"></script>
<script src="/lib/bootstrap/dist/js/bootstrap.js"></script>
<script src="/lib/chartist/dist/chartist.js"></script>
<!-- endbower -->
<!-- inject:css-->
<link rel="stylesheet" href="/css/animate.min.css">
<link rel="stylesheet" href="/css/demo.css">
<link rel="stylesheet" href="/css/light-bootstrap-dashboard.css">
<link rel="stylesheet" href="/css/pe-icon-7-stroke.css">
<link rel="stylesheet" href="/css/styles.css">
<!-- endinject-->
<!-- inject:js-->
<script src="/js/app.js"></script>
<script src="/js/demo.js"></script>
<script src="/js/light-bootstrap-dashboard.js"></script>
<!-- endinject-->
</head>
<style>
body {
	background: white;
}

</style>


<body class="container">

	<div class="row">
		<!-- <I><h2 class="text-center"> Receiving Application</h2></I> -->
		<div class="col-md-offset-1 col-md-6 col-sm-12">
			<div class="login">
				<h2><font color="#222222">Sign-in</font> </h2> 
				<hr>
				<!--Code of form started-->
				<form class="form-horizontal" name="frm" action="/auth/signIn" method="post">
					<!--Code to validate first name is written here-->
					<div class="form-group">
						<label id="label" class="control-label">User Name:</label> <input
							type="text" class="form-control" name="username"
							ng-model="user.username" placeholder="Enter User Name"
							autocomplete="off" required />
					</div>

					<!--Code to validate password is written here-->
					<div class="form-group">
						<label id="label" class="control-label">Password:</label> <input
							type="password" class="in1 form-control" ng-model="user.pass"
							name="pass" placeholder="Enter Password" autocomplete="off"
							ng-minlength="5" ng-maxlength="12" required> 
					</div>

					<div class="form-group">
						<input ng-disabled="frm.$invalid" class="btn btn-primary"
							type="submit" value="Submit">
						<input class="btn btn-primary" type="reset" 
							value="Clear"> 
					</div>
				</form>
			</div>
		</div>
	</div>
</body>
</html>